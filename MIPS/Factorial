.text 
.globl main

main:
	li $a0, 30
	jal fact
	move $a0, $v0
	li $v0, 1
	syscall
	li $v0, 10
	syscall
	
fact:
	subu $sp, $sp, 4
	sw $ra, ($sp)
	beq $a0,0,end
	subu $sp, $sp, 4
	sw $a0, ($sp)
	subu $a0, $a0, 1
	jal fact
	lw $t0, ($sp)
	addu $sp, $sp, 4
	mult $v0, $t0
	mflo $v0
	lw $ra, ($sp)
	addu $sp, $sp, 4
	jr $ra
	
end:
	lw $ra, ($sp)
	addu $sp, $sp, 4
	li $v0, 1
	jr $ra
	
	