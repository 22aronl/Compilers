.text 
.globl main

main:
	li $a0, 1
	li $a1, 2
	li $a2, 3
	jal max3
	move $a0, $v0
	li $v0, 1
	syscall
	li $v0, 10
	syscall
	
max3:
	subu $sp, $sp, 4
	sw $ra, ($sp)
	
	jal max2
	
	move $a0, $v0
	move $a1, $a2
	
	jal max2
	
	lw $ra, ($sp)
	addu $sp, $sp, 4
	jr $ra
max2:
	subu $sp, $sp, 4
	sw $ra, ($sp)
	blt $a0, $a1, next
	move $v0, $a0
	lw $ra, ($sp)
	addu $sp, $sp, 4
	jr $ra
	
next:
	move $v0, $a1
	jr $ra