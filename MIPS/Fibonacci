.text 
.globl main

main:
	li $a0, 10
	jal fact
	move $a0, $v0
	li $v0, 1
	syscall
	li $v0, 10
	syscall
	
fact:
	subu $sp, $sp, 4
	sw $ra, ($sp)
	
	ble $a0,1,end

	subu $sp, $sp, 4
	sw $a0, ($sp)
	
	subu $a0, $a0, 1
	jal fact
	
	lw $t1, ($sp)
	addu $sp, $sp, 4
	
	subu $sp, $sp, 4
	sw $v0, ($sp)
	
	subu $a0, $t1, 2
	jal fact
	
	lw $t0, ($sp)
	addu $sp, $sp, 4
	addu $v0, $v0, $t0
	lw $ra, ($sp)
	addu $sp, $sp, 4
	jr $ra
	
end:
	lw $ra, ($sp)
	addu $sp, $sp, 4
	move $v0, $a0
	jr $ra
	
	
